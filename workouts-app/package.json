    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "jest": "^26.6.3",
    "lint-staged": "^13.1.0",
    "metro-react-native-babel-preset": "0.72.3",
    "prettier": "^2.8.1",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
